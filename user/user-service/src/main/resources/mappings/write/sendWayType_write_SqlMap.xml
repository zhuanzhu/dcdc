<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.user.dao.write.SendWayTypeWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.user.po.SendWayTypePO">
		INSERT INTO u_send_way_type(
        <trim suffixOverrides=",">
			<if test="name != null">
				name,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="name != null">
				#{name},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE u_send_way_type 
		<set>
			<if test="name != null">
				name = #{name},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from u_send_way_type where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from u_send_way_type 
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
		</where>
	</delete>


</mapper>
	