<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.user.dao.write.InfoTemplateParameterWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.user.po.InfoTemplateParameterPO">
		INSERT INTO u_info_template_parameter(
        <trim suffixOverrides=",">
			<if test="infoTemplateId != null">
				info_template_id,
			</if>
			<if test="name != null">
				name,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="infoTemplateId != null">
				#{infoTemplateId},
			</if>
			<if test="name != null">
				#{name},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE u_info_template_parameter 
		<set>
			<if test="infoTemplateId != null">
				info_template_id = #{infoTemplateId},
			</if>			
			<if test="name != null">
				name = #{name},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from u_info_template_parameter where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from u_info_template_parameter 
		<where>
			<if test="infoTemplateId != null">
				and info_template_id = #{infoTemplateId}
			</if>
			<if test="name != null">
				and name = #{name}
			</if>
		</where>
	</delete>


</mapper>
	