<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.user.dao.write.UniauthMenuDomainMapper">
	<resultMap id="UniauthMenuDomainResultMapper" type="com.egeo.components.user.bean.UniauthMenuDomain">
		<result column="mdid" property="mdid"/>
		<result column="mid" property="mid"/>
		<result column="domain_id" property="domainId"/>
	</resultMap>
	
	<sql id="commonColumns">
		 mdid
		, mid
		, domain_id
	</sql>
	
	<sql id="commonCondition">
		<if test="mdid!= null and mdid!= ''"> 
			AND mdid=#{mdid}
		</if>
		<if test="mid!= null and mid!= ''"> 
			AND mid=#{mid}
		</if>
		<if test="domainId!= null and domainId!= ''"> 
			AND domain_id=#{domainId}
		</if>
	</sql>
	
	<insert id="insert" parameterType="com.egeo.components.user.bean.UniauthMenuDomain" keyProperty="mdid" useGeneratedKeys="true">
		<![CDATA[ INSERT INTO uniauth_menu_domain (
				 mid
				, domain_id
				 ) VALUES (
				 #{mid}
				, #{domainId}
  				) ]]>
	</insert>

	<delete id="deleteByProperty" parameterType="com.egeo.components.user.bean.UniauthMenuDomain">
		DELETE FROM uniauth_menu_domain WHERE 1 = 1
		<include refid="commonCondition"/>
	</delete>

	<select id="listByProperty" parameterType="com.egeo.components.user.bean.UniauthMenuDomain" resultMap="UniauthMenuDomainResultMapper">
		<![CDATA[ SELECT ]]>
		<include refid="commonColumns"/>
		FROM uniauth_menu_domain WHERE 1=1
		<include refid="commonCondition"/>
	</select>
</mapper>