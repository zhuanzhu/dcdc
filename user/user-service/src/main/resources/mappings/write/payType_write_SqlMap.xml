<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.user.dao.write.PayTypeWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.user.po.PayTypePO">
		INSERT INTO pay_type(
        <trim suffixOverrides=",">
			<if test="payTypeCode != null">
				pay_type_code,
			</if>
			<if test="payTypeName != null">
				pay_type_name,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
			<if test="logImageUrl != null">
				log_image_url,
			</if>
			<if test="payTypeRemarks != null">
				pay_type_remarks,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="payTypeCode != null">
				#{payTypeCode},
			</if>
			<if test="payTypeName != null">
				#{payTypeName},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
			<if test="logImageUrl != null">
				#{logImageUrl},
			</if>
			<if test="payTypeRemarks != null">
				#{payTypeRemarks},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE pay_type 
		<set>
			<if test="payTypeCode != null">
				pay_type_code = #{payTypeCode},
			</if>			
			<if test="payTypeName != null">
				pay_type_name = #{payTypeName},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>
			<if test="logImageUrl != null">
				log_image_url=#{logImageUrl},
			</if>
			<if test="payTypeRemarks != null">
				pay_type_remarks=#{payTypeRemarks},
			</if>
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from pay_type where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from pay_type 
		<where>
			<if test="payTypeCode != null">
				and pay_type_code = #{payTypeCode}
			</if>
			<if test="payTypeName != null">
				and pay_type_name = #{payTypeName}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
		</where>
	</delete>


</mapper>
	