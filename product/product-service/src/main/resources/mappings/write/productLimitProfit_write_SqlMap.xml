<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.product.dao.write.ProductLimitProfitWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.product.po.ProductLimitProfitPO">
		INSERT INTO product_limit_profit(
        <trim suffixOverrides=",">
			<if test="productLimitProfit != null">
				product_limit_profit,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="productLimitProfit != null">
				#{productLimitProfit},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE product_limit_profit 
		<set>
			<if test="productLimitProfit != null">
				product_limit_profit = #{productLimitProfit},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from product_limit_profit where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from product_limit_profit 
		<where>
			<if test="productLimitProfit != null">
				and product_limit_profit = #{productLimitProfit}
			</if>
		</where>
	</delete>


</mapper>
	