<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.product.dao.write.StandardUnitMembershipWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.product.po.StandardUnitMembershipPO">
		INSERT INTO standard_unit_membership(
        <trim suffixOverrides=",">
			<if test="membershipId != null">
				membership_id,
			</if>
			<if test="standardUnitId != null">
				standard_unit_id,
			</if>
			<if test="platformId != null">
				platform_id,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="membershipId != null">
				#{membershipId},
			</if>
			<if test="standardUnitId != null">
				#{standardUnitId},
			</if>
			<if test="platformId != null">
				#{platformId},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE standard_unit_membership 
		<set>
			<if test="membershipId != null">
				membership_id = #{membershipId},
			</if>			
			<if test="standardUnitId != null">
				standard_unit_id = #{standardUnitId},
			</if>			
			<if test="platformId != null">
				platform_id = #{platformId},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from standard_unit_membership where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from standard_unit_membership 
		<where>
			<if test="membershipId != null">
				and membership_id = #{membershipId}
			</if>
			<if test="standardUnitId != null">
				and standard_unit_id = #{standardUnitId}
			</if>
			<if test="platformId != null">
				and platform_id = #{platformId}
			</if>
		</where>
	</delete>


</mapper>
	