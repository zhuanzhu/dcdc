<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.product.dao.write.AttributeNameWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.product.po.AttributeNamePO">
		INSERT INTO attribute_name(
        <trim suffixOverrides=",">
			<if test="name != null">
				name,
			</if>
			<if test="specificationProperty != null">
				specification_property,
			</if>
			<if test="parameterProperty != null">
				parameter_property,
			</if>
			<if test="platformId != null">
				platform_id,
			</if>
			<if test="mode != null">
				mode,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="name != null">
				#{name},
			</if>
			<if test="specificationProperty != null">
				#{specificationProperty},
			</if>
			<if test="parameterProperty != null">
				#{parameterProperty},
			</if>
			<if test="platformId != null">
				#{platformId},
			</if>
			<if test="mode != null">
				#{mode},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE attribute_name 
		<set>
			<if test="name != null">
				name = #{name},
			</if>			
			<if test="platformId != null">
				platform_id = #{platformId},
			</if>			
			<if test="mode != null">
				mode = #{mode},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>	
			<if test="specificationProperty != null">
				specification_property = #{specificationProperty},
			</if>
			<if test="parameterProperty != null">
				parameter_property = #{parameterProperty},
			</if>		
		</set>
			WHERE id = #{id}
	</update>	
	
	<delete id="delete">
		delete from attribute_name where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from attribute_name 
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="platformId != null">
				and platform_id = #{platformId}
			</if>
			<if test="mode != null">
				and mode = #{mode}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
			<if test="specificationProperty != null">
				specification_property = #{specificationProperty}
			</if>
			<if test="parameterProperty != null">
				parameter_property = #{parameterProperty}
			</if>
		</where>
	</delete>


</mapper>
	