			<if test="updateTime != null">
				#{updateTime},
			</if>
			<if test="templateId != null">
				#{templateId},
			</if>
			<if test="sort != null">
				#{sort},
			</if>
		</trim>
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select
			LAST_INSERT_ID()
		</selectKey>
	</insert>




	<update id="update">
		UPDATE element
		<set>
			<if test="name != null">
				name = #{name},
			</if>
			<if test="type != null">
				type = #{type},
			</if>
			<if test="clientVersionA != null">
				client_version_a = #{clientVersionA},
			</if>
			<if test="clientVersionI != null">
