			<if test="templateId != null">
				template_id=#{templateId},
			</if>
			<if test="sort != null">
				sort=#{sort},
			</if>
		</set>
		WHERE id = #{id}
	</update>


	<delete id="delete">
		delete from element where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from element
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="type != null">
				and type = #{type}
			</if>
