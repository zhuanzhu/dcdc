			</if>
			<if test="configType != null">
				config_type,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
			<if test="templateId != null">
				template_id,
			</if>
			<if test="sort != null">
				sort,
			</if>
		</trim>
		) VALUES (
		<trim suffixOverrides=",">
			<if test="name != null">
				#{name},
			</if>
			<if test="type != null">
				#{type},
			</if>
			<if test="clientVersionA != null">
				#{clientVersionA},
			</if>
			<if test="clientVersionI != null">
				#{clientVersionI},
