<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.cms.dao.write.ElementDictWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.cms.po.ElementDictPO">
		INSERT INTO element_dict(
        <trim suffixOverrides=",">
			<if test="imgUrl != null">
				img_url,
			</if>
			<if test="name != null">
				name,
			</if>
			<if test="configType != null">
				config_type,
			</if>
			<if test="clientVersionA != null">
				client_version_a,
			</if>
			<if test="clientVersionI != null">
				client_version_i,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
			<if test="type != null">
				type,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="imgUrl != null">
				#{imgUrl},
			</if>
			<if test="name != null">
				#{name},
			</if>
			<if test="configType != null">
				#{configType},
			</if>
			<if test="clientVersionA != null">
				#{clientVersionA},
			</if>
			<if test="clientVersionI != null">
				#{clientVersionI},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
			<if test="type != null">
				#{type},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE element_dict 
		<set>
			<if test="imgUrl != null">
				img_url = #{imgUrl},
			</if>			
			<if test="name != null">
				name = #{name},
			</if>			
			<if test="configType != null">
				config_type = #{configType},
			</if>			
			<if test="clientVersionA != null">
				client_version_a = #{clientVersionA},
			</if>			
			<if test="clientVersionI != null">
				client_version_i = #{clientVersionI},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>
			<if test="type != null">
				type=#{type},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from element_dict where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from element_dict 
		<where>
			<if test="imgUrl != null">
				and img_url = #{imgUrl}
			</if>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="configType != null">
				and config_type = #{configType}
			</if>
			<if test="clientVersionA != null">
				and client_version_a = #{clientVersionA}
			</if>
			<if test="clientVersionI != null">
				and client_version_i = #{clientVersionI}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
			<if test="type != null">
				and type=#{type}
			</if>
		</where>
	</delete>


</mapper>
	