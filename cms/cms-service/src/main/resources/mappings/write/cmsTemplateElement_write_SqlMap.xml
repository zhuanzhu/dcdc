<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.cms.dao.write.CmsTemplateElementWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.cms.po.CmsTemplateElementPO">
		INSERT INTO cms_template_element(
        <trim suffixOverrides=",">
			<if test="templateId != null">
				template_id,
			</if>
			<if test="elementId != null">
				element_id,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
			<if test="sort != null">
				sort,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="templateId != null">
				#{templateId},
			</if>
			<if test="elementId != null">
				#{elementId},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
			<if test="sort != null">
				#{sort},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>

	<insert id="insertBatch" parameterType="java.util.List" useGeneratedKeys="false">
		insert into cms_template_element(
		template_id,
		element_id,
		sort
		)values
		<foreach collection="pos" item="po" index="index" separator=",">
		
		(#{po.templateId},#{po.elementId},#{po.sort})
		
		</foreach>
	</insert>
	
	
	
	<update id="update">
	UPDATE cms_template_element 
		<set>
			<if test="templateId != null">
				template_id = #{templateId},
			</if>			
			<if test="elementId != null">
				element_id = #{elementId},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>			
			<if test="sort != null">
				sort = #{sort},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from cms_template_element where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from cms_template_element 
		<where>
			<if test="templateId != null">
				and template_id = #{templateId}
			</if>
			<if test="elementId != null">
				and element_id = #{elementId}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
			<if test="sort != null">
				and sort = #{sort}
			</if>
		</where>
	</delete>


</mapper>
	