<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.cms.dao.write.ExternalLinkWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.cms.po.ExternalLinkPO">
		INSERT INTO external_link(
        <trim suffixOverrides=",">
			<if test="name != null">
				name,
			</if>
			<if test="externalLinkUrl != null">
				external_link_url,
			</if>
			<if test="linkType != null">
				link_type,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="name != null">
				#{name},
			</if>
			<if test="externalLinkUrl != null">
				#{externalLinkUrl},
			</if>
			<if test="linkType != null">
				#{linkType},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE external_link 
		<set>
			<if test="name != null">
				name = #{name},
			</if>			
			<if test="externalLinkUrl != null">
				external_link_url = #{externalLinkUrl},
			</if>			
			<if test="linkType != null">
				link_type = #{linkType},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from external_link where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from external_link 
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="externalLinkUrl != null">
				and external_link_url = #{externalLinkUrl}
			</if>
			<if test="linkType != null">
				and link_type = #{linkType}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
		</where>
	</delete>


</mapper>
	