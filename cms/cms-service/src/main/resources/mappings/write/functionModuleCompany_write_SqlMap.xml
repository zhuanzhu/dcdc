<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.cms.dao.write.FunctionModuleCompanyWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.cms.po.FunctionModuleCompanyPO">
		INSERT INTO function_module_company(
        <trim suffixOverrides=",">
			<if test="functionModuleId != null">
				function_module_id,
			</if>
			<if test="companyId != null">
				company_id,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="functionModuleId != null">
				#{functionModuleId},
			</if>
			<if test="companyId != null">
				#{companyId},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE function_module_company 
		<set>
			<if test="functionModuleId != null">
				function_module_id = #{functionModuleId},
			</if>			
			<if test="companyId != null">
				company_id = #{companyId},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from function_module_company where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from function_module_company 
		<where>
			<if test="functionModuleId != null">
				and function_module_id = #{functionModuleId}
			</if>
			<if test="companyId != null">
				and company_id = #{companyId}
			</if>
		</where>
	</delete>


</mapper>
	