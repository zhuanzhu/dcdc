<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.cms.dao.write.InstWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.cms.po.InstPO">
		INSERT INTO inst(
        <trim suffixOverrides=",">
			<if test="name != null">
				name,
			</if>
			<if test="instMargin != null">
				inst_margin,
			</if>
			<if test="elementId != null">
				element_id,
			</if>
			<if test="configType != null">
				config_type,
			</if>
			<if test="pageTabId != null">
				page_tab_id,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="name != null">
				#{name},
			</if>
			<if test="instMargin != null">
				#{instMargin},
			</if>
			<if test="elementId != null">
				#{elementId},
			</if>
			<if test="configType != null">
				#{configType},
			</if>
			<if test="pageTabId != null">
				#{pageTabId},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE inst 
		<set>
			<if test="name != null">
				name = #{name},
			</if>			
			<if test="instMargin != null">
				inst_margin = #{instMargin},
			</if>			
			<if test="elementId != null">
				element_id = #{elementId},
			</if>			
			<if test="configType != null">
				config_type = #{configType},
			</if>			
			<if test="pageTabId != null">
				page_tab_id = #{pageTabId},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from inst where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from inst 
		<where>
			<if test="name != null">
				and name = #{name}
			</if>
			<if test="instMargin != null">
				and inst_margin = #{instMargin}
			</if>
			<if test="elementId != null">
				and element_id = #{elementId}
			</if>
			<if test="configType != null">
				and config_type = #{configType}
			</if>
			<if test="pageTabId != null">
				and page_tab_id = #{pageTabId}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
		</where>
	</delete>

	<update id="resetInstPageTabInfo">
		UPDATE inst set page_tab_id=-1 where page_tab_id = #{po.pageTabId}
	</update>

</mapper>
	