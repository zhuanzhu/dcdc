<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.config.dao.write.LogOperCodeWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.config.po.LogOperCodePO">
		INSERT INTO log_oper_code(
        <trim suffixOverrides=",">
			<if test="operCode != null">
				oper_code,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="updateTime != null">
				update_time,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="operCode != null">
				#{operCode},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="updateTime != null">
				#{updateTime},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE log_oper_code 
		<set>
			<if test="operCode != null">
				oper_code = #{operCode},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="updateTime != null">
				update_time = #{updateTime},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from log_oper_code where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from log_oper_code 
		<where>
			<if test="operCode != null">
				and oper_code = #{operCode}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="updateTime != null">
				and update_time = #{updateTime}
			</if>
		</where>
	</delete>


</mapper>
	