<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.config.dao.write.PaymentCodeSaltWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.config.po.PaymentCodeSaltPO">
		INSERT INTO payment_code_salt(
        <trim suffixOverrides=",">
			<if test="paymentCodeUuid != null">
				payment_code_uuid,
			</if>
			<if test="saltValue != null">
				salt_value,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="paymentCodeUuid != null">
				#{paymentCodeUuid},
			</if>
			<if test="saltValue != null">
				#{saltValue},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE payment_code_salt 
		<set>
			<if test="paymentCodeUuid != null">
				payment_code_uuid = #{paymentCodeUuid},
			</if>			
			<if test="saltValue != null">
				salt_value = #{saltValue},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from payment_code_salt where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from payment_code_salt 
		<where>
			<if test="paymentCodeUuid != null">
				and payment_code_uuid = #{paymentCodeUuid}
			</if>
			<if test="saltValue != null">
				and salt_value = #{saltValue}
			</if>
		</where>
	</delete>


</mapper>
	