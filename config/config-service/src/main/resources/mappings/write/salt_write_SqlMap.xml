<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.config.dao.write.SaltWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.config.po.SaltPO">
		INSERT INTO salt(
        <trim suffixOverrides=",">
			<if test="uuid != null">
				uuid,
			</if>
			<if test="saltValue != null">
				salt_value,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="uuid != null">
				#{uuid},
			</if>
			<if test="saltValue != null">
				#{saltValue},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE salt 
		<set>
			<if test="uuid != null">
				uuid = #{uuid},
			</if>			
			<if test="saltValue != null">
				salt_value = #{saltValue},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from salt where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from salt 
		<where>
			<if test="uuid != null">
				and uuid = #{uuid}
			</if>
			<if test="saltValue != null">
				and salt_value = #{saltValue}
			</if>
		</where>
	</delete>


</mapper>
	