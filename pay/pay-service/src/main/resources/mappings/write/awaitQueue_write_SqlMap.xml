<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.egeo.components.pay.dao.write.AwaitQueueWriteDAO">
	<insert id="insert" parameterType="com.egeo.components.pay.po.AwaitQueuePO">
		INSERT INTO await_queue(
        <trim suffixOverrides=",">
			<if test="soId != null">
				so_id,
			</if>
			<if test="orderCode != null">
				order_code,
			</if>
			<if test="randomNumber != null">
				random_number,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
			<if test="cashPayType != null">
				cash_pay_type,
			</if>
        </trim>		
		) VALUES (
        <trim suffixOverrides=",">
			<if test="soId != null">
				#{soId},
			</if>
			<if test="orderCode != null">
				#{orderCode},
			</if>
			<if test="randomNumber != null">
				#{randomNumber},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
			<if test="cashPayType != null">
				#{cashPayType},
			</if>
        </trim>		
		)
		<selectKey keyProperty="id" resultType="java.lang.Long">
			select LAST_INSERT_ID()
		</selectKey>
	</insert>


	
	
	<update id="update">
	UPDATE await_queue 
		<set>
			<if test="soId != null">
				so_id = #{soId},
			</if>			
			<if test="orderCode != null">
				order_code = #{orderCode},
			</if>			
			<if test="randomNumber != null">
				random_number = #{randomNumber},
			</if>			
			<if test="createTime != null">
				create_time = #{createTime},
			</if>			
			<if test="cashPayType != null">
				cash_pay_type = #{cashPayType},
			</if>			
		</set>
			WHERE id = #{id}
	</update>	
	

	<delete id="delete">
		delete from await_queue where id = #{id}
	</delete>

	<delete id="deleteByPara">
		delete from await_queue 
		<where>
			<if test="soId != null">
				and so_id = #{soId}
			</if>
			<if test="orderCode != null">
				and order_code = #{orderCode}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="cashPayType != null">
				and cash_pay_type = #{cashPayType}
			</if>
		</where>
	</delete>
	<update id="updateAwaitQueueBySoIdWithTx">
		UPDATE await_queue set is_pay_true = #{po.isPayTrue} WHERE so_id = #{po.soId}
	</update>	

</mapper>
	